{
  "name": "hull-processor",
  "description": "Run code to update User Properties and generate Events",
  "version": "0.2.8",
  "homepage": "https://github.com/hull-ships/hull-processor",
  "license": "MIT",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.12.x",
    "npm": "3.10.x",
    "yarn": "1.3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-processor"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-processor/issues"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain hull-processor",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "babel server -d lib",
    "start": "node -r newrelic ./lib",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug babel-watch -L server",
    "test": "npm run test:lint && npm run test:units && npm run test:specs",
    "test:lint": "eslint server",
    "test:modules": "npm outdated --depth=0",
    "test:units": "NODE_ENV=test mocha --require babel-register -R spec ./tests/unit/*.js",
    "test:specs": "NODE_ENV=test mocha --require babel-register -R spec ./tests/integration/*.js",
    "clean": "rimraf dist; rimraf lib",
    "update": "updtr",
    "watch": "watch 'clear && npm run test:lint -s && npm run test:units -s && npm run build:client -s' server src",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship"
  ],
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "cache-manager-redis": "^0.5.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.33.0",
    "connect": "^3.4.1",
    "connect-timeout": "^1.7.0",
    "eslint": "^4.15.0",
    "css-loader": "^0.28.9",
    "deep-diff": "^0.3.4",
    "deep-freeze": "0.0.1",
    "deepmerge": "^2.0.1",
    "event-stream": "^3.3.3",
    "express": "^4.14.0",
    "hull": "0.13.10",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.13.1",
    "moment": "^2.20.1",
    "newrelic": "^2.6.0",
    "raven": "^2.3.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "request": "^2.74.0",
    "response-time": "^2.3.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.19.1",
    "superagent": "^3.8.2",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.0.0",
    "syntax-error": "^1.3.0",
    "urijs": "^1.18.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "winston-logstash": "^0.4.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "hull-connector-tester": "^0.0.5",
    "jwt-decode": "^2.2.0",
    "minihull": "^2.1.0",
    "mocha": "^5.0.0",
    "react-hot-loader": "^3.1.3",
    "sinon": "^4.1.6",
    "supertest": "^3.0.0",
    "updtr": "^2.0.0",
    "watch": "^1.0.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4"
  }
}
